#!/bin/sh -e

#DEBHELPER#

if [ "$1" != purge ] ; then
	exit 0
fi

# Remove obsolete user.
userdel franklin3d > /dev/null 2> /dev/null || :
groupdel franklin3d > /dev/null 2> /dev/null || :

# Remove current user.
userdel _franklin > /dev/null 2> /dev/null || :
groupdel _franklin > /dev/null 2> /dev/null || :
